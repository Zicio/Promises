{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-unused-vars": "off",
    "no-plusplus": "off",
    "max-classes-per-file": "off",
    "no-underscore-dangle": "off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}